{
  "name": "@luxcium/parallel-mapper",
  "version": "0.0.0-PREALPHA-UNSAFE-v0.0x",
  "license": "MIT",
  "private": false,
  "description": "Parallel processing library with CPU and I/O mappers",
  "main": "./lib/src/index.js",
  "types": "./lib/typings/index.d.ts",
  "engines": {
    "node": ">=22"
  },
  "isLibrary": true,
  "scripts": {
    "build": "tsc --build tsconfig.json",
    "clean": "rimraf lib coverage",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint \"src/**/*.ts\" \"tests/**/*.ts\"",
    "lint:fix": "eslint \"src/**/*.ts\" \"tests/**/*.ts\" --fix",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "type-check": "tsc --noEmit",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "keywords": [
    "parallel",
    "async",
    "mapping",
    "worker-threads",
    "promise",
    "typescript"
  ],
  "_comment": "⚠️ NEVER EDIT DEPENDENCIES DIRECTLY! use rush add with -m flag and one -p flag per package and one -D flag for the dev deps.",
  "author": "Benjamin Vincent (Luxcium)",
  "devDependencies": {
    "@babel/core": "~7.23.9",
    "@babel/plugin-transform-react-jsx": "~7.22.5",
    "@babel/plugin-transform-typescript": "~7.25.9",
    "@eslint-stylistic/metadata": "~1.6.1",
    "@jest/globals": "~29.7.0",
    "@rushstack/heft-jest-plugin": "~0.10.6",
    "@rushstack/heft": "~0.63.4",
    "@types/chai": "~4.3.5",
    "@types/eslint-config-prettier": "~6.11.3",
    "@types/eslint-plugin-prettier": "~3.1.3",
    "@types/express": "~5.0.1",
    "@types/fs-extra": "~11.0.1",
    "@types/glob": "~8.1.0",
    "@types/heft-jest": "~1.0.3",
    "@types/jest": "~29.5.12",
    "@types/mime-types": "~2.1.1",
    "@types/node": "~22.14.0",
    "@types/sharp": "^0.30.0",
    "am": "^1.1.0",
    "chai": "~4.3.7",
    "concurrently": "~8.0.1",
    "eslint-config-prettier": "~9.1.0",
    "eslint-import-resolver-typescript": "~3.6.3",
    "eslint-plugin-import": "~2.31.0",
    "eslint-plugin-jsdoc": "~48.0.5",
    "eslint-plugin-jsonc": "~2.13.0",
    "eslint-plugin-prettier": "~5.2.1",
    "eslint-plugin-simple-import-sort": "~12.1.1",
    "eslint-plugin-tsdoc": "~0.2.17",
    "eslint-plugin-unicorn": "~51.0.0",
    "got": "^12.0.2",
    "jest": "~29.7.0",
    "nodemon": "~3.0.3",
    "prettier": "~3.4.2",
    "rimraf": "~6.0.1",
    "ts-jest": "~29.2.5",
    "ts-node": "~10.9.2",
    "tsconfig-paths": "~4.2.0",
    "tslib": "~2.6.2",
    "typescript": "~5.8.3",
    "prettier-plugin-organize-attributes": "~1.0.0",
    "prettier-plugin-packagejson": "~2.5.2",
    "prettier-plugin-sh": "~0.14.0",
    "eslint": "~9.24.0",
    "@eslint/js": "~9.12.0",
    "@types/eslint__js": "~8.42.3",
    "typescript-eslint": "~8.12.2"
  },
  "dependencies": {
    "dotenv": "~16.5.0",
    "fp-ts": "~2.13.1",
    "heft-jest": "~1.0.2",
    "source-map-support": "~0.5.21"
  }
